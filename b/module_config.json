{
  "author": "jamesfchen",
  "description": "该配置文件给idea plugin插件和gradle plugin读取,idea plugin通过该文件可以实现管理想要编译的模块，gradle plugin则可以根据这些模块信息生成快速模块",
  "buildVariants": [
    "debug",
    "release"
  ],
  "groupId": "io.github.jamesfchen",
  "allModules": [
    {
      "simpleName": "app",
      "sourcePath": ":app",
      "format": "app",
      "group": "father"
    },
    {
      "simpleName": "home-myhome",
      "sourcePath": ":home-myhome",
      "format": "bundle",
      "group": "home"
    },
    {
      "simpleName": "hotel-main",
      "sourcePath": ":hotel-module:main",
      "format": "app",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-bundle1",
      "sourcePath": ":hotel-module:bundle1",
      "format": "bundle",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-bundle2",
      "sourcePath": ":hotel-module:bundle2",
      "format": "bundle",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-bundle3",
      "sourcePath": ":hotel-module:bundle3",
      "format": "bundle",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-bundle4",
      "sourcePath": ":hotel-module:bundle4",
      "format": "bundle",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-lint",
      "sourcePath": ":hotel-module:hotel-lint",
      "format": "jar",
      "type": "tool",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-foundation",
      "sourcePath": ":hotel-module:foundation",
      "format": "bundle_foundation",
      "group": "hotel"
    },
    {
      "simpleName": "hotel-export",
      "sourcePath": ":hotel-module:export",
      "format": "bundle_foundation",
      "group": "hotel"
    },
    {
      "simpleName": "framework-baseui",
      "sourcePath": ":framework:baseui",
      "format": "framework_foundation",
      "group": "fwk"
    },
    {
      "simpleName": "framework-network",
      "sourcePath": ":framework:network",
      "format": "framework_foundation",
      "group": "fwk"
    },
    {
      "simpleName": "framework-h5container",
      "sourcePath": ":framework:h5container",
      "format": "framework_foundation",
      "group": "fwk"
    },
    {
      "simpleName": "framework-reactcontainer",
      "sourcePath": ":framework:reactcontainer",
      "format": "framework_foundation",
      "group": "fwk"
    },
    {
      "simpleName": "framework-common",
      "sourcePath": ":framework:common",
      "format": "framework_foundation",
      "group": "fwk"
    },
    {
      "simpleName": "im-main",
      "sourcePath": ":im-module:main",
      "format": "app",
      "group": "im",
      "dynamic": "remote_plugin"
    },
    {
      "simpleName": "im-chatroom",
      "sourcePath": ":im-module:chatroom",
      "format": "bundle",
      "group": "im",
      "dynamic": "local_plugin"
    }
  ]
}