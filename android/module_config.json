      "binary_artifact": "com.jamesfchen.b:framework-loader:1.0",
      "deps": [
        ":framework:network"
      ]
    },
    {
      "simpleName": "framework-network",
      "canonicalName": ":framework:network",
      "format": "framework_foundation",
      "group": "fwk",
      "binary_artifact": "com.jamesfchen.b:framework-network:1.0",
      "deps": [
        ":framework:common"
      ]
    },
    {
      "simpleName": "framework-common",
      "canonicalName": ":framework:common",
      "format": "framework_foundation",
      "group": "fwk",
      "binary_artifact": "com.jamesfchen.b:framework-common:1.0"
