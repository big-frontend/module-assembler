{
  "author": "jamesfchen",
  "description": "该配置文件给idea plugin插件和gradle plugin读取,idea plugin通过该文件可以实现管理想要编译的模块，gradle plugin则可以根据这些模块信息生成快速模块",
  "allModules": [
    {
      "simpleName": "app",
      "canonicalName": ":app",
      "format": "app",
      "group": "father",
      "binary_artifact": "com.jamesfchen.b:app:1.0",
      "deps": [
        ":hotel-module:bundle1",":hotel-module:bundle2", ":home-module:myhome"
      ]
    },
    {
      "simpleName": "home-main",
      "canonicalName": ":home-module:main",
      "format": "app",
      "group": "home",
      "binary_artifact": "com.jamesfchen.b:home-main:1.0",
      "deps": [
        ":home-module:myhome"
      ]
    },
    {
      "simpleName": "home-myhome",
      "canonicalName": ":home-module:myhome",
      "format": "app",
      "group": "home",
      "binary_artifact": "com.jamesfchen.b:home-myhome:1.0",
      "deps": [
        ":framework:loader"
      ]
    },
    {
      "simpleName": "hotel-main",
      "canonicalName": ":hotel-module:main",
      "format": "app",
      "group": "hotel",
      "binary_artifact": "com.jamesfchen.b:hotel-main:1.0",
